// EASE/HDL begin //////////////////////////////////////////////////////////////
// 
// module 'Serial'.
// 
////////////////////////////////////////////////////////////////////////////////

module Serial (Rx, Tx, PT, Clk, Rst, Result, En, Ry) ;
  input      Rx;
  output     Tx;
  output     PT;
  input      Clk;
  input      Rst;
  input      Result;
  input      En;
  output     Ry;


// EASE/HDL end ////////////////////////////////////////////////////////////////



endmodule // Serial
